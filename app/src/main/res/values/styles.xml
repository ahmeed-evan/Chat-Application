<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

    <style name="Rubik_Black_14">
        <item name="android:textColor">@color/colorBlack</item>
        <item name="fontFamily">@font/rubik</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Rubik_light_Black_14">
        <item name="android:textColor">@color/colorBlack</item>
        <item name="fontFamily">@font/rubik</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Rubik_Medium_White_14">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="fontFamily">@font/rubik_medium</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Rubik_Medium_Black_16">
        <item name="android:textColor">@color/colorBlack</item>
        <item name="fontFamily">@font/rubik_medium</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Rubik_Light_Black_16">
        <item name="android:textColor">@color/colorBlack</item>
        <item name="fontFamily">@font/rubik_light</item>
        <item name="android:textSize">16sp</item>
    </style>

</resources>
